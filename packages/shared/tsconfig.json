{
  "compilerOptions": {
  "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "esModuleInterop": true,
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}